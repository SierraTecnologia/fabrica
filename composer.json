{
    "name": "sierratecnologia/fabrica",
    "description": "Fabrica de Projetos",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "sierratecnologia/crypto": "dev-master",
        "sierratecnologia/integrations": "dev-master",
        "sierratecnologia/telefonica": "dev-master",
        "deployer/deployer": ">=6.4",
        "vanderlee/php-sentence": ">=1.0",
        "fzaninotto/faker": "*",
        "php-http/guzzle6-adapter": "*",
        "devio/pipedrive": "*",
        "league/flysystem-aws-s3-v3": "*",
        "league/plates": "^3.1|^4.0",
        "geerlingguy/ping": "*",
        "phpbench/phpbench": "*",
        "phploc/phploc": "*",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "phpro/grumphp": "*",
        "phpseclib/phpseclib": "*",
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "*",
        "aws/aws-sdk-php": "*",
        "predis/predis": "*",
        "sebastian/diff": "*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "Fabrica\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Fabrica\\FabricaProvider"
            ]
        }
    }
}
